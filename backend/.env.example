# file: backend/.env.example
# Copy to backend/.env and fill in GEMINI_API_KEY

# Folder with your PDFs/DOCX
DOCS_DIR=./docs

# Where to write/read the JSONL index
INDEX_PATH=./data/index.jsonl

# Embeddings backend ("fastembed" default). You can switch later.
EMBEDDINGS_BACKEND=fastembed
# FastEmbed model
EMBED_MODEL_NAME=BAAI/bge-small-en-v1.5

# Retrieval
TOP_K=8

# Gemini 
GEMINI_API_KEY=
GEMINI_MODEL=gemini-3-pro-preview